{
  "name": "biblioteca-virtual",
  "version": "v1.0.1",
  "private": true,
  "author": "Jordy Zenteno",
  "description": "Virtual bookshelf",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "publish": "electron-builder build --win --publish always"
  },
  "build": {
    "appId": "BibliotecaVirtual",
    "extraResources": [
      "assets"
    ],
    "win": {
      "icon": "build/icon.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.3",
    "fs": "^0.0.1-security",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0"
  }
}